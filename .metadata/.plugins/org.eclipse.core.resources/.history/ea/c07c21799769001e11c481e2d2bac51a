package com.cg;

public class Sum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String num1 = "20";
	    String num2 = "30";
	    String num3 = "40";
	    String num4 = "50";
	    
	    String concatenated = num1 + num2 + num3 + num4;
	    int sum = Integer.parseInt(concatenated);
	    System.out.println("sum: " + sum);
	    
	}

}
